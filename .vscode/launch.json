{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Master/Node",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/${fileBasenameNoExtension}.exe",
        "args": [
            "--src_ip=192.168.1.132",
            "--dst_ip=192.168.1.134",
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build cpp files",
        "miDebuggerPath": "/usr/bin/gdb"
      }
    ]
  }
  